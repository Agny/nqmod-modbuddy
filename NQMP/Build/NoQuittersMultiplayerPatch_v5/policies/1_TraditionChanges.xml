<GameData>
	
	<!-- Swap Legalism's requirement to Aristocracy (instead of Oligarchy) -->
	<Policy_PrereqPolicies>
		<Update>
			<Set PrereqPolicy="POLICY_ARISTOCRACY" />
			<Where PolicyType="POLICY_LEGALISM" />
		</Update>
	</Policy_PrereqPolicies>
	<Policies>
		<Update>
			<Set GridX="3" GridY="1" />
			<Where Type="POLICY_ARISTOCRACY" />
		</Update>
		<Update>
			<Set GridX="1" GridY="1" />
			<Where Type="POLICY_OLIGARCHY" />
		</Update>
	</Policies>
	
</GameData>